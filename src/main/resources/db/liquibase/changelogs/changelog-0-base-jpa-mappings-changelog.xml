<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

  <!-- TODO: Table definitions are to be added here -->

  <!-- ADDRESS table definition -->
  <changeSet id="CL0-CS01" author="Priyanjana Rodrigo">
    <createTable tableName="ADDRESS">
      <column name="ID" type="UUID">
        <constraints nullable="false" primaryKey="true" primaryKeyName="ADDRESS_PK"/>
      </column>
      <column name="RESIDENCE_NO" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="ADDRESS_LINE_1" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="ADDRESS_LINE_2" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="CITY" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
    </createTable>
  </changeSet>

</databaseChangeLog>
